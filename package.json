{
  "name": "nestjs-mangopay",
  "version": "3.0.0",
  "description": "Injectable Mangopay client for Nestjs",
  "license": "MIT",
  "author": {
    "name": "Nabeel Hayat",
    "email": "nabeel.hayat87@gmail.com"
  },
  "repository": "https://github.com/nabeelhayat/nestjs-mangopay",
  "bugs": "https://github.com/nabeelhayat/nestjs-mangopay/issues",
  "private": false,
  "keywords": [
    "nestjs",
    "mangopay",
    "mangopay",
    "sdk",
    "credit cards",
    "api",
    "typescript",
    "nestjs-mangopay",
    "mangopay-nestjs"
  ],
  "source": "lib/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build:dist": "yarn clean:dist && tsc -p tsconfig.build.json",
    "test:run": "NODE_ENV=test jest",
    "test:watch": "yarn test:run --watch",
    "test:coverage": "yarn clean:test && yarn test:run --coverage",
    "type-check": "tsc --noEmit",
    "lint": "eslint --fix . --ext .js,.ts,.tsx",
    "clean:dist": "rimraf dist",
    "clean:test": "rimraf coverage",
    "prepare": "yarn build:dist",
    "postinstall": "husky install"
  },
  "engines": {
    "npm": "please-use-yarn",
    "yarn": ">=1.22.19",
    "node": ">=16.0.0"
  },
  "dependencies": {
    "mangopay2-nodejs-sdk": "^1.33.0"
  },
  "peerDependencies": {
    "@nestjs/common": ">=9.0.0",
    "@nestjs/core": ">=9.0.0"
  },
  "devDependencies": {
    "@nestjs/common": "9.2.1",
    "@nestjs/core": "9.2.1",
    "@nestjs/testing": "9.2.1",
    "@types/jest": "29.4.0",
    "@types/node": "18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "dotenv": "^16.0.3",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.2",
    "jest": "29.4.0",
    "lint-staged": "^13.0.3",
    "prettier": "2.8.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.0.0",
    "rxjs": "7.8.0",
    "ts-jest": "29.0.5",
    "typescript": "4.9.4"
  }
}
